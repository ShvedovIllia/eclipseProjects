package main;

public class Animal {
	private String type;
	private int age;
	private String name;
	public Animal(String type, int age, String name) {
		this.type = type;
		this.age = age;
		this.name = name;
	}
	public Animal () {
		
	}
	@Override
	public String toString() {
		return "Animal [type=" + type + ", age=" + age + ", name=" + name + "]";
	}
	
	
	
	

}
